<% if user_signed_in? %>
  <li>
    <%= link_to edit_user_registration_path do %>
      <%= fa_icon 'user' %> Manage Account
    <% end %>
  </li>
<% else %>
  <% if !Rails.application.config.restricted_registration %>
  <li>
    <%= link_to new_user_registration_path do %>
      <%= fa_icon 'user' %> Register
    <% end %>
  </li>
  <% end %>
<% end %>
